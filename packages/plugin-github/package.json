{
    "title": "GitHub",
    "name": "@jeff-tian/gitbook-plugin-github",
    "publishConfig": {
        "access": "public"
    },
    "version": "1.8.5",
    "icon": "./icon.png",
    "description": "Integrate your website with your GitHub repository.",
    "main": "index.js",
    "browser": "./_assets/plugin.js",
    "authors": [
        {
            "name": "Michael Jackson"
        },
        {
            "name": "Samy PessÃ©",
            "email": "samy@gitbook.com"
        }
    ],
    "devDependencies": {
        "eslint": "^3.7.1",
        "eslint-config-gitbook": "^1.5.0",
        "gitbook-plugin": "^4.0.0-alpha.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/GitbookIO/plugin-github.git"
    },
    "license": "Apache-2.0",
    "engines": {
        "gitbook": ">=1.8.2"
    },
    "keywords": [
        "gitbook:social"
    ],
    "scripts": {
        "build-js": "gitbook-plugin build ./src/index.js ./_assets/plugin.js",
        "prepublish": "npm run build-js"
    },
    "gitbook": {
        "properties": {
            "url": {
                "type": "string",
                "required": true,
                "title": "URL of your GitHub repository"
            }
        }
    }
}
